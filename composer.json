{
  "name": "reflar/clean-profile-posts",
  "description": "Removes the initial posts of discussions from the user's profile posts list",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta-7"
  },
  "authors": [
    {
      "name": "David Sevilla Martin",
      "email": "david.s@redevs.org",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Reflar\\CleanProfilePosts\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "ReFlar Clean Profile Posts",
      "icon": {
        "name": "far fa-file-code",
        "backgroundColor": "#263238",
        "color": "#FFF"
      }
    },
    "flagrow": {
      "discuss": "https://discuss.flarum.org/d/16137-clean-profile-posts-by-reflar"
    }
  }
}
